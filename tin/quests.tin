#action {^Anarwen says 'Would you help me, {\w+}?'$} {
	#showme {Anarwen says...};
	#if {"%1" == "$NAME"} {
		#showme {say yes}; say yes;
		#delay {.5} {say yes};
	};
};

#alias {willow} {#showme hunting place:w 1:n 2:w 3:w 4:s 5:w 6:w 7:n 8:w dangerous:n deep:e deep:e willow.}
#alias {kementir} {#showme say valinor, telperion, laurelin}


};

#alias {showmode} {
    #if {"$MODAL" == "on"} {
	   #if {"$MODE" == "none"} {#showme {<029>$MODAL<099>> } {0}};
	   #else {#showme {<029>$MODAL<099>> $MODE} {0}};
	};
	#else {
	   #showme {<019>$MODAL<099>> } {0};
	};
};


